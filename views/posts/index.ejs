<div id="container">
    <h2>Hi
        <%= username %>
    </h2>
</div>

<div id="container">
    <a href="/add/new">New post</a>
    <a href="/logout">Logout</a>

    <h2>@
        <%= username %> posts</h2>

    <% posts.forEach(function(post) { %>
        <div class="post-container">

            <p class="post-author">

                <%= name %>
            </p>

            <p class="post-title">
                Title
                <%= post.title %>
            </p>
            <p class="post content">
                Content
                <%= post.content %>
            </p>
            <p class="date">
                <%= moment(post.created_at).format("LL, LTS") %>
            </p>
        </div>
        <% }) %>
</div>